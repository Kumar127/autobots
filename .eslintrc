{
  "rules": {
    "linebreak-style":"off",
    "no-console": 0,
    "no-underscore-dangle": 0,
    "no-unused-vars": ["warn", { "argsIgnorePattern": "next" }],
    "no-use-before-define": ["error", { "variables": false }],
    "no-multi-str": 0,
    "max-len": [ "error", {"code": 140}],
    "arrow-body-style":"off",
    "import/no-unresolved": "warn"
  },
  "env": {
    "node": true,
    "mocha": true
  },
  "parserOptions": {
    "ecmaVersion": 8
  },
  "extends": [
    "airbnb-base",
    "prettier"
  ]
}